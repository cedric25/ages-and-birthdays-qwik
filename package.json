{
  "name": "my-qwik-basic-starter",
  "description": "Recommended for your first Qwik app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "watch.tailwind": "npx tailwindcss -i ./src/global.css -o ./dist/output.css --watch",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "dependencies": {
    "dayjs": "1.11.5",
    "firebase": "9.10.0",
    "lodash.deburr": "4.1.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.9.0",
    "@builder.io/qwik-city": "0.0.111",
    "@types/eslint": "8.4.6",
    "@types/lodash.deburr": "4.1.7",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "8.23.1",
    "eslint-plugin-qwik": "0.9.0",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.3",
    "vite": "3.1.1",
    "vite-tsconfig-paths": "3.5.0"
  }
}
